@model List<DTO.EmployeeShift>
<div class="form-group">
    <div class="row">
        <div>
            <span class="col-sm-4 sm-offset-1"><label class="control-label">Date From :</label></span>
            <span class="col-sm-3">
                @Html.TextBox("DateFrom","",new { @class="form-control" ,Type="Date"})
            </span>
        </div>
    </div>
</div>
<div class="form-group">
    <div class="row">
        <div>
            <span class="col-sm-4 sm-offset-1"><label class="control-label">Date To :</label></span>
            <span class="col-sm-3">
                @Html.TextBox("DateTo","",new { @class="form-control" ,Type="Date"})
            </span>
        </div>
    </div>
</div>
<table class="table table-striped table-bordered table-hover dataTables-example" id="shifttable">
    <thead>
        <tr>
            <th style="text-align:center">Name</th>
            <th style="text-align:center">Shift</th>
        </tr>
    </thead>
    <tbody>
       @foreach (var employee in Model)
        {
        <tr>
            <td>@employee.EmployeeID</td>
            <td>@employee.ShiftTypeID</td>
        </tr>
        }
    </tbody>
</table>